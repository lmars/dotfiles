[user]
  name = Lewis Marshall
  email = lewismarshall86@gmail.com
[alias]
  co = checkout
  c  = commit -v
  ca = commit -v --amend
  s  = status
  l  = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --all --abbrev-commit --date=relative
  d  = diff
  dc = diff --cached
  a  = add
  ap = add --patch
[core]
  excludesfile = ~/.gitignore
  editor = gvim -f -c 'startinsert'
[color]
  ui = true
